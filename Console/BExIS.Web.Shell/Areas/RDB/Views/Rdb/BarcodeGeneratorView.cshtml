@model List<BExIS.Modules.Rdb.UI.Models.BarCodeLabelModel>
@{
    ViewBag.Title = "BarcodeGeneratorView";

}
<!--A4 210 x 297 mm - a7 52 x 74-->
<div class="a4-page">
    @foreach (var barcodeLabel in Model)
    {
        @Html.Partial("BarcodeLabelView", barcodeLabel)
    }
</div>


<style>

    .a4-page {
        border: 0.1mm #red;
        border-style: groove;
        width: 210mm;
        height: 297mm;
        
    }

    .barcode-label {
        border: 0.1mm dotted #000000;
        width: 90mm;
        height: 54mm;
        margin: 5mm;
    }

    .barcode-label-text-container {
        padding: 20pt 10pt 20pt 20pt;
    }

    .barcode-label-id {
        width: 100%;
        text-align: center;
        font-size: 15pt;
    }

    .barcode-label-plot {
        width: 100%;
        text-align: center;

    }

    .barcode {
        width: 100%;
        padding-top: 15pt;
        text-align: center;
    }
    .qrcode {
        margin: -28mm 0 0 63mm;
    }
</style>